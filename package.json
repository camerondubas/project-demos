{
  "name": "angular-starter-project",
  "license": "MIT",
  "private": false,
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "webpack-dev-server --history-api-fallback --inline --progress --port=8080",
    "build": "webpack --config --bail -p --progress --production && docker build -t user/image:version ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/camerondubas/angular-starter-project.git"
  },
  "eslintConfig": {
    "extends": "google",
    "ecmaVersion": 6,
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "require-jsdoc": 0,
      "no-warning-comments": 0,
      "no-return-assign": 0,
      "no-useless-constructor": 0,
      "max-len": [
        1,
        250,
        2
      ],
      "max-nested-callbacks": 0
    },
    "globals": {
      "document": false,
      "window": false,
      "localStorage": false,
      "Image": false
    }
  },
  "dependencies": {
    "angular": "~1.5.0",
    "angular-resource": "~1.5.0",
    "angular-ui-router": "~0.3.1",
    "autoprefixer": "~6.3.3",
    "babel-core": "~6.9.1",
    "babel-loader": "~6.2.4",
    "babel-preset-es2015": "~6.9.0",
    "css-loader": "~0.23.1",
    "eslint": "~2.12.0",
    "eslint-config-google": "~0.6.0",
    "eslint-loader": "~1.3.0",
    "extract-text-webpack-plugin": "~0.9.1",
    "file-loader": "~0.8.4",
    "html-loader": "~0.4.0",
    "html-webpack-plugin": "~1.7.0",
    "ng-annotate-loader": "^0.1.1",
    "ngtemplate-loader": "^1.3.1",
    "node-libs-browser": "~0.5.3",
    "node-sass": "~3.8.0",
    "null-loader": "~0.1.1",
    "postcss-loader": "~0.8.1",
    "raw-loader": "~0.5.1",
    "sass-loader": "~3.1.2",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.14"
  },
  "devDependencies": {
    "webpack-dev-server": "~1.14.1"
  }
}
